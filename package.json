{
		"name": "alvs-service-template-v2",
		"version": "1.0.0",
		"main": "./dist/server.js",
		"repository": "git@github.com:ALVS-Company/alvs-service-template-v2.git",
		"author": "Gabriel Alves <gabriel.alves@alvstech.com>",
		"license": "MIT",
		"scripts": {
				"clean": "rm -Rf dist",
				"build": "npm run clean && npx swc src -d dist",
				"compile": "npx swc src -q -w -d dist",
				"start": "NODE_ENV=production node ./dist/server.js",
				"watch": "nodemon --watch \"./dist/**/*\" ./dist/server.js",
				"dev": "concurrently \"npm run compile\" \"npm run watch\"",
				"lint": "eslint ./src",
				"pretty-quick": "pretty-quick --staged",
				"prepare": "husky install"
		},
		"devDependencies": {
				"@commitlint/cli": "^17.0.3",
				"@commitlint/config-conventional": "^17.0.3",
				"@swc/cli": "^0.1.57",
				"@swc/core": "^1.2.189",
				"@tsconfig/node16": "^1.0.2",
				"@types/jsonwebtoken": "^8.5.8",
				"@types/kcors": "^2.2.6",
				"@types/koa": "^2.13.4",
				"@types/koa-bodyparser": "^4.3.7",
				"@types/koa-router": "^7.4.4",
				"@types/node": "^17.0.34",
				"@typescript-eslint/eslint-plugin": "^5.25.0",
				"@typescript-eslint/parser": "^5.25.0",
				"concurrently": "^7.2.1",
				"eslint": "^8.15.0",
				"husky": "^8.0.1",
				"pretty-quick": "^3.1.3",
				"ts-node": "^10.7.0",
				"tsc-alias": "^1.6.7",
				"tsconfig-paths": "^4.0.0",
				"typescript": "^4.6.4"
		},
		"dependencies": {
				"@types/koa-better-body": "^3.3.3",
				"@uswitch/koa-timeout": "^1.6.0",
				"axios": "^0.27.2",
				"cls-rtracer": "^2.6.2",
				"dotenv": "^16.0.1",
				"ioredis": "^5.0.6",
				"jsonwebtoken": "^8.5.1",
				"jwt-decode": "^3.1.2",
				"kcors": "^2.2.2",
				"koa": "^2.13.4",
				"koa-better-body": "^3.3.9",
				"koa-bodyparser": "^4.3.0",
				"koa-helmet": "^6.1.0",
				"koa-router": "^10.1.1",
				"mongoose": "^6.3.6",
				"prettier": "^2.7.1",
				"winston": "^3.8.2",
				"yup": "^0.32.11"
		}
}
